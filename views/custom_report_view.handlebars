<div id="c_view_reports" data-view="custom" data-mode="reports" style="height: 100%;display: none;">
        <div id="c_view_reports_title">
            <h1>Reportes</h1>
        </div>
        <div id="c_view_reports_layout" style="height: 100%;">
            <aside id="c_layout_aside">
                <h3>Grupos</h3>
                <div class="layout_items">
                    <div class="input_cheack_group">
                        <input type="checkbox" name="" id="main_all_check_input" value="ALL">
                        <label for="main_all_check_input">TODOS</label>
                    </div>
                    {{#each meshs}}
                        <div class="input_cheack_group">
                            <input value="{{_id}}" type="checkbox" name="mesh_input" id="{{ _id }}">
                            <label for="{{ _id }}">{{ name }}</label>
                        </div>
                    {{/each}}
                </div>
            </aside>
            <form id="c_layout_filters">
                <div class="input_group">
                    <label for="c_layout_select">EPAD</label>
                    <select required disabled name="c_layout_select" id="c_layout_select" style="width: 150px;">
                        <option>-</option>
                    </select>
                </div>
                <div class="input_group">
                    <label>Fecha de sinconizaci√≥n</label>
                    <div class="group_items">
                        <div class="input_box">
                            <label for="start_date">Desde</label>
                            <input required type="date" name="start_date" id="start_date">
                        </div>
                        <div class="input_box">
                            <label for="end_date">Hasta</label>
                            <input required type="date" name="end_date" id="end_date">
                        </div>
                    </div>
                </div>
                <div class="input_group">
                    <label>Listar por</label>
                    <select required name="c" id="c_select_pagination" style="width: 150px;">
                        <option value="5">5</option>
                        <option value="10">10</option>
                        <option value="20">20</option>
                        <option value="50">50</option>
                        <option value="100">100</option>
                    </select>
                </div>
                <div class="input_group">
                    <label></label>
                    <input style="cursor: pointer;" type="submit" value="Buscar" id="search_devices">
                </div>
            </form>
            <div id="c_layout_content">
                <h3>Seleccione un grupo</h3>
            </div>
        </div>
    </div>